{
  "name": "react-component-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for fast development of reusable and self-contained React component.",
  "engines": {
    "node": ">=5.2"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "clean": "rm -rf ./target ./lib ./dist",
    "babel": "babel -d lib src --ignore=spec.js",
    "babelDev": "watch 'npm run babel' ./src",
    "build": "npm run clean && npm run babel && webpack --config webpack.prod.config.js",
    "test": "karma start",
    "testDev": "karma start --no-single-run --browsers Chrome,PhantomJS"
  },
  "main": "lib/js/index.js",
  "author": "Bill Gao",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.0.15",
    "bower": "^1.7.2",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.4",
    "karma-junit-reporter": "^0.3.8",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfills": "0.0.1",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "watch": "^0.17.1",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.5.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "React",
    "React Component",
    "Reusable Component",
    "Boilerplate",
    "Hot reloading",
    "Karma",
    "Webpack",
    "Fast development"
  ]
}
